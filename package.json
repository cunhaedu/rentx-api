    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "jest": "^27.4.4",
    "prettier": "^2.4.1",
    "ts-jest": "^27.1.1",
    "ts-node-dev": "^1.1.8",
    "tsconfig-paths": "^3.11.0",
    "tscpaths": "^0.0.9",
    "typescript": "^4.4.4"
